// KMP 
// Complexity: O(n + m)
// Source: http://www.shafaetsplanet.com/?p=3209

vector<int> lps;

void kmp(string p, string t) {
  string s = p + '?' + t;
  int n = (int) s.size();
  lps.resize(n);
  lps[0] = 0;
  for (int i = 1; i < n; i++) {
    int j = lps[i - 1];
    while(j > 0 and s[i] != s[j]) {
      j = lps[j - 1];
    }
    lps[i] = (s[i] == s[j]) ? j + 1 : j;
  }
  return lps;
}
